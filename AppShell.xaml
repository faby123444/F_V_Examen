<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="F_V_Examen.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:F_V_Examen"
    Shell.FlyoutBehavior="Disabled">

    <ShellItem Route="main">
        <ShellSection Title="Home">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </ShellSection>
        <ShellSection Title="Centro Costos">
            <ShellContent ContentTemplate="{DataTemplate local:CentroCostosPage}" />
        </ShellSection>
    </ShellItem>

</Shell>

